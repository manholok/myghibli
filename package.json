{
  "name": "myghibli",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "clean": "rimraf -- dist",
    "test": "npm run test:unit",
    "test:unit": "karma start",
    "build": "npm run clean && npm run build:dev",
    "build:dev": "npm run clean && webpack --config config/webpack.dev.js",
    "build:prod": "npm run clean && webpack --config config/webpack.prod.js",
    "build:prod:sourcemap": "npm run clean && webpack --config config/webpack.sourcemap.js",
    "server": "npm run server:dev",
    "server:dev": "npm run clean && webpack-dev-server --config config/webpack.dev.js --open --progress --profile --watch",
    "server:prod": "npm run clean && webpack-dev-server --config config/webpack.prod.js --open --progress --profile --watch",
    "server:prod:sourcemap": "npm run clean && webpack-dev-server --output-pathinfo --config config/webpack.sourcemap.js --open --progress --profile --watch "
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/manholok/myghibli.git"
  },
